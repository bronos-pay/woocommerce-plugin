{
  "name": "bronos-pay/bronos-pay-php",
  "description": "Bronos Pay library for PHP",
  "keywords": [
    "bronos-pay",
    "bronos",
    "bitcoin",
    "litecoin",
    "altcoin",
    "merchant",
    "gateway",
    "payment"
  ],
  "homepage": "https://payments.bronos.org",
  "license": "MIT",
  "authors": [
    {
      "name": "Bronos Pay and contributors",
      "homepage": "https://github.com/bronos-pay/bronos-pay-php/graphs/contributors"
    }
  ],
  "require": {
    "php": ">=7.3.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "~3.6.2",
    "phpstan/phpstan": "~1.4.10"
  },
  "autoload": {
    "psr-4": {
      "BronosPay\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BronosPay\\": [
        "tests/",
        "tests/BronosPay/"
      ]
    }
  }
}
